{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "src/*": ["./*"],
      "src": ["."]
    }
  },
  "include": [
    "next-env.d.ts",
    "typings/**/*.d.ts",
    ".eslintrc.js",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx"
  ],
  "exclude": ["node_modules"]
}
